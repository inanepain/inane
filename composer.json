{
	"name": "inanepain/inane",
	"description": "Offers: File Metadata, Server http file with resume, Enum, zf2 config reader, observer pattern, zf3 custom config autoload and string tools, array merge",
	"type": "library",
	"keywords": [
		"http",
		"debug",
		"time",
		"server",
		"file",
		"metadata",
		"config",
		"enum",
		"resume",
		"string"
	],
	"homepage": "https://git.inane.co.za:3000/inane/tools",
	"license": "MIT",
	"authors": [{
		"name": "Philip Michael Raab",
		"email": "philip@inane.co.za",
		"homepage": "https://inane.co.za",
		"role": "Developer"
	}],
	"support": {
		"email": "philip@inane.co.za",
		"wiki": "https://git.inane.co.za:3000/inane/tools/wiki",
		"issues": "https://git.inane.co.za:3000/inane/tools/issues"
	},
	"require": {
		"php": ">=8.0",
        "psr/container": "*",
        "psr/http-message": "*",
		"laminas/laminas-http": "*"
	},
	"autoload": {
		"psr-4": {
			"Inane\\": "src/"
		}
	},
	"minimum-stability": "dev",
	"repositories": [{
		"type": "composer",
		"url": "https://packer.inane.co.za",
		"name": "Inane Packer"
	}],
	"config": {
		"preferred-install": "dist",
		"optimize-autoloader": true,
		"discard-changes": true,
		"sort-packages": true
	}
}
