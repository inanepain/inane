{
	"name": "inanepain/inane",
	"description": "Offers: File Metadata, Server http file with resume, Enum, zf2 config reader, observer pattern, zf3 custom config autoload and string tools, array merge",
	"type": "library",
	"keywords": [
		"inane",
		"time",
		"file",
		"metadata",
		"config",
		"string"
	],
	"homepage": "http://vaieen.local:8000/inane/tools",
	"license": "UNLICENSE",
	"readme": "README.md",
	"authors": [
		{
			"name": "Philip Michael Raab",
			"email": "philip@inane.co.za",
			"homepage": "https://inane.co.za",
			"role": "Developer"
		}
	],
	"support": {
		"email": "philip@inane.co.za",
		"wiki": "https://git.inane.co.za:3000/inane/tools/wiki",
		"issues": "https://git.inane.co.za:3000/inane/tools/issues"
	},
	"_comment": [
		{
			"inane-lib": {
				"inanepain/auth": "*",
				"inanepain/db": "*",
				"inanepain/cli": "*",
				"inanepain/crypt": "*",
				"inanepain/dumper": "*",
				"inanepain/event": "*",
				"inanepain/http": "*",
				"inanepain/redis": "*",
				"inanepain/routing": "*",
				"inanepain/stdlib": "*",
				"inanepain/view": "*"
			}
		}
	],
	"autoload": {
		"psr-4": {
			"Inane\\": "src/"
		}
	},
	"config": {
		"preferred-install": "dist",
		"optimize-autoloader": true,
		"discard-changes": true,
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"require": {
		"php": ">=8.1",
		"psr/container": "*",
		"laminas/laminas-http": "*"
	}
}
